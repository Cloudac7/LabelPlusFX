      {
        "groupId": 0,
        "index": 24,
        "text": "拿过来！",
        "x": 0.439,
        "y": 0.756
      },
      {
        "groupId": 0,
        "index": 25,
        "text": "啥！？",
        "x": 0.203,
        "y": 0.9
      }
